{
  "apiVersion": "v1",
  "kind": "PersistentVolumeClaim",
  "metadata": {
    "name": "gluster-default-volume"
  },
  "spec": {
    "capacity": {
      "storage": "2Gi"
    },
    "accessModes": [ "ReadWriteMany" ],
    "glusterfs": {
      "endpoints": "glusterfs-cluster",
      "path": "myVol1"
    }
  }
}